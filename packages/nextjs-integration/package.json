{
  "name": "@useward/instrumentation",
  "version": "0.1.0",
  "description": "Next.js instrumentation SDK for full-stack observability",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/useward/ward.git",
    "directory": "packages/nextjs-integration"
  },
  "homepage": "https://github.com/useward/ward#readme",
  "bugs": {
    "url": "https://github.com/useward/ward/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware.d.ts",
      "import": "./dist/middleware.js",
      "default": "./dist/middleware.js"
    },
    "./client": {
      "types": "./dist/instrumentation.client.d.ts",
      "import": "./dist/instrumentation.client.js",
      "default": "./dist/instrumentation.client.js"
    },
    "./server-react": {
      "types": "./dist/server-react.d.ts",
      "import": "./dist/server-react.js",
      "default": "./dist/server-react.js"
    },
    "./turbopack-config": {
      "types": "./dist/turbopack-config.d.ts",
      "import": "./dist/turbopack-config.js",
      "default": "./dist/turbopack-config.js"
    },
    "./session-meta": {
      "types": "./dist/session-meta.d.ts",
      "import": "./dist/session-meta.js",
      "default": "./dist/session-meta.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/context-zone": "^2.2.0",
    "@opentelemetry/core": "^2.2.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/instrumentation-fetch": "^0.208.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/instrumentation-undici": "^0.19.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.208.0",
    "@opentelemetry/otlp-transformer": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-web": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@vercel/otel": "^2.1.0",
    "hono": "^4.10.7",
    "tsdown": "^0.16.7",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@useward/shared": "workspace:*",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "vitest": "^4.0.16",
    "webpack": "^5.103.0"
  },
  "peerDependencies": {
    "next": ">=15.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": false
    },
    "react": {
      "optional": false
    }
  },
  "keywords": [
    "nextjs",
    "next",
    "observability",
    "opentelemetry",
    "otel",
    "tracing",
    "performance",
    "monitoring",
    "instrumentation",
    "rsc",
    "server-components"
  ],
  "author": "PerfoTech, LLC",
  "license": "FSL-1.1-Apache-2.0",
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.11.1"
}
